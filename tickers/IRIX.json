[{"address1": "1212 Terra Bella Avenue", "city": "Mountain View", "state": "CA", "zip": "94043", "country": "United States", "phone": "650 940 4700", "fax": "650 940 4710", "website": "https://www.iridex.com", "industry": "Medical Devices", "industryKey": "medical-devices", "industryDisp": "Medical Devices", "sector": "Healthcare", "sectorKey": "healthcare", "sectorDisp": "Healthcare", "longBusinessSummary": "IRIDEX Corporation, an ophthalmic medical technology company, provides therapeutic based laser systems, delivery devices, and consumable instrumentation to treat sight-threatening eye diseases in ophthalmology. It offers laser consoles, such as Cyclo G6 laser system for use in the treatment of glaucoma; IQ 532 and IQ 577 laser systems, which are used for the treatment of retinal disorders; and OcuLight TX, OcuLight SL, OcuLight SLx, OcuLight GL, and OcuLight GLx laser photocoagulation systems that are used to treat proliferative diabetic retinopathy, macular holes, retinal tears, and detachments. The company also provides delivery devices, including TxCell scanning laser delivery system that allows the physician to perform multi-spot pattern scanning; slit lamp adapter, which allows the physician to utilize a standard slit lamp in diagnosis and treatment procedures; and laser indirect ophthalmoscope for use in procedures to treat peripheral retinal disorders. It offers MicroPulse P3 Probe, which is used with its Cylco G6 laser system to perform MicroPulse transscleral laser therapy; G-Probe, which is used in procedures to treat uncontrolled glaucoma; G-Probe and G-Probe Illuminate, which are used in procedures to treat refractory glaucoma; and EndoProbe family of products for use in vitrectomy procedures. The company serves ophthalmologists, research and teaching hospitals, government installations, surgical centers, hospitals, veterinary practices, and office clinics. It markets its products through direct and independent sales force in the United States, as well as through independent distributors internationally. The company was formerly known as IRIS Medical Instruments, Inc. and changed its name to IRIDEX Corporation in November 1995. IRIDEX Corporation was incorporated in 1989 and is headquartered in Mountain View, California.", "fullTimeEmployees": 111, "companyOfficers": [{"maxAge": 1, "name": "Mr. Scott A. Shuda J.D.", "age": 59, "title": "Executive Chairman", "yearBorn": 1965, "fiscalYear": 2023, "totalPay": 79000, "exercisedValue": 0, "unexercisedValue": 0}, {"maxAge": 1, "name": "Mr. Patrick  Mercer", "age": 51, "title": "CEO, President & Director", "yearBorn": 1973, "fiscalYear": 2023, "totalPay": 365848, "exercisedValue": 0, "unexercisedValue": 70318}, {"maxAge": 1, "name": "Mr. Romeo R. Dizon", "age": 66, "title": "Chief Financial Officer", "yearBorn": 1958, "fiscalYear": 2023, "totalPay": 241323, "exercisedValue": 0, "unexercisedValue": 0}, {"maxAge": 1, "name": "Ms. Leigh  Salvo", "title": "Head of Investor Relations", "fiscalYear": 2023, "exercisedValue": 0, "unexercisedValue": 0}], "compensationAsOfEpochDate": 1703980800, "irWebsite": "http://phx.corporate-ir.net/phoenix.zhtml?c=112360&p=irol-irhome", "executiveTeam": [], "maxAge": 86400, "priceHint": 4, "previousClose": 0.8863, "open": 1.06, "dayLow": 0.9101, "dayHigh": 1.0652, "regularMarketPreviousClose": 0.8863, "regularMarketOpen": 1.06, "regularMarketDayLow": 0.9101, "regularMarketDayHigh": 1.0652, "payoutRatio": 0.0, "beta": 0.944, "forwardPE": -3.7003703, "volume": 449503, "regularMarketVolume": 449503, "averageVolume": 70083, "averageVolume10days": 177670, "averageDailyVolume10Day": 177670, "bid": 0.0, "ask": 0.0, "bidSize": 0, "askSize": 0, "marketCap": 16621428, "fiftyTwoWeekLow": 0.78, "fiftyTwoWeekHigh": 3.65, "priceToSalesTrailing12Months": 0.34319812, "fiftyDayAverage": 1.3682, "twoHundredDayAverage": 1.71225, "trailingAnnualDividendRate": 0.0, "trailingAnnualDividendYield": 0.0, "currency": "USD", "tradeable": false, "enterpriseValue": 1