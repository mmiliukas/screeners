[{"address1": "1212 Terra Bella Avenue", "city": "Mountain View", "state": "CA", "zip": "94043", "country": "United States", "phone": "650 940 4700", "fax": "650 940 4710", "website": "https://www.iridex.com", "industry": "Medical Devices", "industryKey": "medical-devices", "industryDisp": "Medical Devices", "sector": "Healthcare", "sectorKey": "healthcare", "sectorDisp": "Healthcare", "longBusinessSummary": "IRIDEX Corporation, an ophthalmic medical technology company, provides therapeutic based laser systems, delivery devices, and consumable instrumentation to treat sight-threatening eye diseases in ophthalmology. It offers laser consoles, such as Cyclo G6 laser system for use in the treatment of glaucoma; IQ 532 and IQ 577 laser systems, which are used for the treatment of retinal disorders; and OcuLight TX, OcuLight SL, OcuLight SLx, OcuLight GL, and OcuLight GLx laser photocoagulation systems that are used to treat proliferative diabetic retinopathy, macular holes, retinal tears, and detachments. The company also provides delivery devices, including slit lamp adapter, which allows the physician to utilize a standard slit lamp in diagnosis and treatment procedures; TxCell scanning laser delivery system that allows the physician to perform multi-spot pattern scanning; and laser indirect ophthalmoscope for use in procedures to treat peripheral retinal disorders. In addition, it offers MicroPulse P3 Probe, which is used with its Cylco G6 laser system to perform transscleral laser therapy; G-Probe and G-Probe Illuminate, which are used in procedures to treat refractory glaucoma; and EndoProbe family of products that are used for endophotocoagulation. The company serves ophthalmologists, research and teaching hospitals, government installations, surgical centers, hospitals, veterinary practices, and office clinics through direct and independent sales force in the United States, as well as through independent distributors internationally. It operates in the United States, Europe, the Middle East, Africa, the Asia/Pacific region, and the Americas. The company was formerly known as IRIS Medical Instruments, Inc. and changed its name to IRIDEX Corporation in November 1995. IRIDEX Corporation was incorporated in 1989 and is headquartered in Mountain View, California.", "fullTimeEmployees": 93, "companyOfficers": [{"maxAge": 1, "name": "Mr. Scott A. Shuda J.D.", "age": 60, "title": "Executive Chairman", "yearBorn": 1965, "fiscalYear": 2024, "totalPay": 84756, "exercisedValue": 0, "unexercisedValue": 0}, {"maxAge": 1, "name": "Mr. Patrick  Mercer", "age": 53, "title": "CEO, President & Director", "yearBorn": 1972, "fiscalYear": 2024, "totalPay": 523842, "exercisedValue": 0, "unexercisedValue": 0}, {"maxAge": 1, "name": "Mr. Bassem  BouHabib", "age": 49, "title": "Vice President of International Sales", "yearBorn": 1976, "fiscalYear": 2024, "totalPay": 453707, "exercisedValue": 0, "unexercisedValue": 0}, {"maxAge": 1, "name": "Mr. Romeo R. Dizon", "age": 67, "title": "Chief Financial Officer", "yearBorn": 1958, "fiscalYear": 2024, "totalPay": 241323, "exercisedValue": 0, "unexercisedValue": 0}, {"maxAge": 1, "name": "Ms. Leigh  Salvo", "title": "Head of Investor Relations", "fiscalYear": 2024, "exercisedValue": 0, "unexercisedValue": 0}], "compensationAsOfEpochDate": 1735603200, "irWebsite": "http://phx.corporate-ir.net/phoenix.zhtml?c=112360&p=irol-irhome", "executiveTeam": [], "maxAge": 86400, "priceHint": 4, "previousClose": 1.35, "open": 1.35, "dayLow": 1.35, "dayHigh": 1.45, "regularMarketPreviousClose": 1.35, "regularMarketOpen": 1.35, "regularMarketDayLow": 1.35, "regularMarketDayHigh": 1.45, "payoutRatio": 0.0, "beta": 0.539, "forwardPE": -5.222222, "volume": 42792, "regularMarketVolume": 42792, "averageVolume": 89445, "averageVolume10days": 118200, "averageDailyVolume10Day": 118200, "bid": 1.37, "ask": 1.48, "bidSize": 1, "askSize": 1, "marketCap": 24237872, "fiftyTwoWeekLow": 0.78, "fiftyTwoWeekHigh": 1.72, "allTimeHigh": 18.