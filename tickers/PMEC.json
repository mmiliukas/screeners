[{"address1": "23 Ubi Crescent", "city": "Singapore", "zip": "408579", "country": "Singapore", "phone": "65 6286 1868", "website": "https://www.primechholdings.com", "industry": "Specialty Business Services", "industryKey": "specialty-business-services", "industryDisp": "Specialty Business Services", "sector": "Industrials", "sectorKey": "industrials", "sectorDisp": "Industrials", "longBusinessSummary": "Primech Holdings Ltd. provides facilities and stewarding services for the public and private sectors in Singapore. The company offers conservancy areas cleaning services, including daily cleaning of common areas within town councils, monthly block washing, and quarterly fogging of bin chutes; educational institutions cleaning services, such as cleaning of schools; airport cleaning services; cleaning of hotels, public spaces and roads, condominium common areas and facilities, offices, industrial areas, and retail stores; and marble polishing services, external fa\u00e7ade cleaning for exteriors of buildings, and stewarding and clean room cleaning services. It also provides waste management services, such as the collection, transportation, and disposal of general waste, and sorting of waste for recycling, as well as pest control services comprising the remediation and prevention of infestations of rodents, insects, birds, and other pests. In addition, the company offers horticultural maintenance services, such as weeding, fertilizing, mowing, and irrigation; stewarding services, including cleaning of kitchen equipment and dishwashing, room attendant services, and public area cleaning services to healthcare facilities, hotels and other hospitality venues, and restaurants; and cleaning services to offices. Further, it operates HomeHelpy, an online portal that allows individual customers to book cleaning services in homes and offices; and manufactures and sells cleaning supplies, such as hand soaps, hand soap dispensers, cleaning fluids, and garbage bags used for general, floor, carpet, restroom, or kitchen purposes, as well as treatment products used in the marine industry under the D'Bond brand name. The company was incorporated in 2020 and is headquartered in Singapore. Primech Holdings Ltd. is a subsidiary of Sapphire Universe Holdings Limited.", "fullTimeEmployees": 2806, "companyOfficers": [{"maxAge": 1, "name": "Mr. Kin Wai  Ho", "age": 48, "title": "CEO & Director", "yearBorn": 1976, "fiscalYear": 2025, "totalPay": 680000, "exercisedValue": 0, "unexercisedValue": 0}, {"maxAge": 1, "name": "Ms. Kit Yu  Lee", "age": 38, "title": "Chief Financial Officer", "yearBorn": 1986, "fiscalYear": 2025, "totalPay": 176000, "exercisedValue": 0, "unexercisedValue": 0}, {"maxAge": 1, "name": "Mr. Yew Jin  Sng", "age": 74, "title": "Senior VP of Business Development & Director", "yearBorn": 1950, "fiscalYear": 2025, "totalPay": 177000, "exercisedValue": 0, "unexercisedValue": 0}, {"maxAge": 1, "name": "Mr. Hansel  Loo", "age": 44, "title": "Senior Vice President of Operations", "yearBorn": 1980, "fiscalYear": 2025, "exercisedValue": 0, "unexercisedValue": 0}, {"maxAge": 1, "name": "Mr. Khazid  bin Omar", "age": 59, "title": "Senior Vice President of Corporate Services", "yearBorn": 1965, "fiscalYear": 2025, "exercisedValue": 0, "unexercisedValue": 0}], "compensationAsOfEpochDate": 1767139200, "executiveTeam": [], "maxAge": 86400, "priceHint": 4, "previousClose": 1.7, "open": 1.7, "dayLow": 1.66, "dayHigh": 1.7, "regularMarketPreviousClose": 1.7, "regularMarketOpen": 1.7, "regularMarketDayLow": 1.66, "regularMarketDayHigh": 1.7, "payoutRatio": 0.0, "volume": 3832, "regularMarketVolume": 3832, "averageVolume": 99807, "averageVolume10days": 40200, "averageDailyVolume10Day": 40200, "bid": 0.0, "ask": 1.72, "bidSize": 0, "askSize": 1, "marketCap": 63773860, "fiftyTwoWeekLow": 0.536, "fiftyTwoWeekHigh": 1.89, "allTimeHigh": 4.18, "allTimeLow": 0.49, "priceToSalesTrailing12Months": 0.8577635, "fiftyDayAverage": 1.6735, "twoHundredDayAverage": 1.16