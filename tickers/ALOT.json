[{"address1": "600 East Greenwich Avenue", "city": "West Warwick", "state": "RI", "zip": "02893", "country": "United States", "phone": "401 828 4000", "fax": "401 822 2430", "website": "https://www.astronovainc.com", "industry": "Computer Hardware", "industryKey": "computer-hardware", "industryDisp": "Computer Hardware", "sector": "Technology", "sectorKey": "technology", "sectorDisp": "Technology", "longBusinessSummary": "AstroNova, Inc. designs, develops, manufactures, and distributes specialty printers, and data acquisition and analysis systems in the United States, Europe, Canada, Asia, Central and South America, and internationally. The company offers color label tabletop printers and light commercial label printers, direct-to-package printers, high-volume presses, specialty original equipment manufacturer printing systems, label printers, envelope and packaging printing, related processing and handling equipment, data acquisition recorders, as well as label, and tag and other supplies, including ink, toner, and thermal transfer ribbons under the QuickLabel, TrojanLabel, GetLabels, Astro Machine, and MTEX brands; and provides on-site and remote service, spare parts, and various service contracts; training and support services; and develops and licenses software programs to design and manage labels, print images, manage and operate printers. It also provides airborne printer products, such as ToughWriter series used in the flight decks of military transport, commercial transport, and business aircraft to print hard copies of data; ToughSwitch, an ethernet switches to connect multiple computers or ethernet devices; TMX and TMX-200, a high-speed data acquisition systems for applications requiring high channel counts and acquisition rates; Daxus DXS-100, a distributed data acquisition platform; SmartCorder DDX-100, a portable all-in-one data acquisition system; and Everest EV-5000 and RC-300 digital strip chart recording systems for aerospace and defense applications. In addition, the company offers networking solutions; aerospace products comprising flight deck printing solutions, networking hardware, and specialized aerospace-grade thermal paper; and data acquisition systems. The company was formerly known as Astro-Med, Inc. and changed its name to AstroNova, Inc. in May 2016. AstroNova, Inc. was incorporated in 1969 and is headquartered in West Warwick, Rhode Island.", "fullTimeEmployees": 441, "companyOfficers": [{"maxAge": 1, "name": "Mr. Michael J. Natalizia", "age": 60, "title": "VP of Technical & Strategic Alliances and CTO", "yearBorn": 1964, "fiscalYear": 2025, "totalPay": 265924, "exercisedValue": 0, "unexercisedValue": 0}, {"maxAge": 1, "name": "Mr. Thomas Wayne Carll", "age": 57, "title": "Senior Vice President of AstroNova Aerospace Segment", "yearBorn": 1967, "fiscalYear": 2025, "totalPay": 233472, "exercisedValue": 0, "unexercisedValue": 0}, {"maxAge": 1, "name": "Mr. Darius G. Nevin", "age": 66, "title": "Executive Chairman", "yearBorn": 1958, "fiscalYear": 2025, "exercisedValue": 0, "unexercisedValue": 0}, {"maxAge": 1, "name": "Mr. Jorik E. Ittmann", "title": "President, CEO & Director", "fiscalYear": 2025, "exercisedValue": 0, "unexercisedValue": 0}, {"maxAge": 1, "name": "Mr. Thomas D. DeByle CPA", "age": 64, "title": "VP, Treasurer & CFO", "yearBorn": 1960, "fiscalYear": 2025, "exercisedValue": 0, "unexercisedValue": 0}, {"maxAge": 1, "name": "Mr. Padraig  Finn", "title": "Senior VP of Product Identification (Product ID)", "fiscalYear": 2025, "exercisedValue": 0, "unexercisedValue": 0}, {"maxAge": 1, "name": "Daniel S. Clevenger", "title": "Secretary", "fiscalYear": 2025, "exercisedValue": 0, "unexercisedValue": 0}], "compensationAsOfEpochDate": 1767139200, "irWebsite": "http://phx.corporate-ir.net/phoenix.zhtml?c=120530&p=irol-irhome", "executiveTeam": [], "maxAge": 86400, "priceHint": 2, "previousClose": 9.86, "open": 9.5, "dayLow": 9.5, "dayHigh": 9.77, "regularMarketPreviousClose": 9.86, "regularMarke