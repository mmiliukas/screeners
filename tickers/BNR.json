[{"address1": "No. 5, Xingdao Ring Road North", "address2": "International Bio Island", "city": "Guangzhou", "zip": "510005", "country": "China", "phone": "86 20 3403 7871", "website": "https://www.brbiotech.com", "industry": "Diagnostics & Research", "industryKey": "diagnostics-research", "industryDisp": "Diagnostics & Research", "sector": "Healthcare", "sectorKey": "healthcare", "sectorDisp": "Healthcare", "longBusinessSummary": "Burning Rock Biotech Limited primarily develops and sells cancer therapy selection tests in the People's Republic of China. It operates through three segments: Central Laboratory Business, In-Hospital Business, and Pharma Research and Development Services. The company offers next-generation sequencing (NGS) based tissue and liquid biopsy cancer therapy selection and prognosis prediction tests for various cancer types, including pan-cancer, lung cancer, gastrointestinal cancer, prostate cancer, breast cancer, lymphomas, thyroid cancer, and ovarian cancer. Its key products include OncoScreen Focus, a multi-gene tumor mutation co-detection test kit for non-small cell lung cancer (NSCLC) patients; OncoScreen Plus, a pan-cancer test for tissue samples, and OncoCompass, a corresponding test for liquid biopsy samples; OncoCompass Target, a ctDNA liquid biopsy-based test for NSCLC; and ColonCore, which assesses microsatellite loci related to MSI status and detects mutations in genes associated with gastrointestinal cancers. It also provides CanCatch, a pre-operative ctDNA detection and post-operative minimal residual disease calling for relapsed patients; OncoMaster, an automatic NGS data analysis and report interpretation machine; as well as Magnis BR-customized version of the key products. The company has a development and commercialization agreement with Myriad Genetics, Inc. to in-license Myriad myChoice tumor testing in China; and collaborations on clinical trials and research studies with AstraZeneca, Bayer, Johnson & Johnson, CStone, BeiGene, Abbisko Therapeutics, IMPACT Therapeutics, Boehringer Ingelheim, and Merck KGaA. Burning Rock Biotech Limited was founded in 2014 and is headquartered in Guangzhou, the People's Republic of China.", "fullTimeEmployees": 674, "companyOfficers": [{"maxAge": 1, "name": "Mr. Yusheng  Han", "age": 45, "title": "Founder, Chairman & CEO", "yearBorn": 1979, "exercisedValue": 0, "unexercisedValue": 0}, {"maxAge": 1, "name": "Ms. Xiaozhi  Hu", "age": 40, "title": "Senior Director of Finance", "yearBorn": 1984, "exercisedValue": 0, "unexercisedValue": 0}, {"maxAge": 1, "name": "Dr. Zhihong  Zhang", "age": 48, "title": "CTO & Director", "yearBorn": 1976, "exercisedValue": 0, "unexercisedValue": 0}, {"maxAge": 1, "name": "Mr. Hao  Liu", "age": 50, "title": "Senior Advisor", "yearBorn": 1974, "exercisedValue": 0, "unexercisedValue": 0}], "executiveTeam": [], "maxAge": 86400, "priceHint": 4, "previousClose": 3.16, "open": 3.2, "dayLow": 3.1512, "dayHigh": 3.22, "regularMarketPreviousClose": 3.16, "regularMarketOpen": 3.2, "regularMarketDayLow": 3.1512, "regularMarketDayHigh": 3.22, "payoutRatio": 0.0, "beta": 0.745, "forwardPE": -5.4496555, "volume": 6187, "regularMarketVolume": 6187, "averageVolume": 41440, "averageVolume10days": 25550, "averageDailyVolume10Day": 25550, "bid": 2.28, "ask": 4.22, "bidSize": 2, "askSize": 2, "marketCap": 34031068, "fiftyTwoWeekLow": 2.18, "fiftyTwoWeekHigh": 7.9, "priceToSalesTrailing12Months": 0.065034024, "fiftyDayAverage": 3.01432, "twoHundredDayAverage": 4.55922, "trailingAnnualDividendRate": 0.0, "trailingAnnualDividendYield": 0.0, "currency": "USD", "tradeable": false, "enterpriseValue": -112738832, "profitMargins": -0.45593, "floatShares": 48307975, "sharesOutstanding": 9034110, "sharesShort": 41457, "sharesShortPriorMonth": 38221, "sharesShortPreviousMonthDate": 1747267200, "dateShortInterest": 1749772800, "sharesPercentSharesOut": 0.0039, "heldPercentInsiders": 0.03918, "heldPercentInstitutions": 0.59860003, "shortRatio": 0.91, "shortP